{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "plugin-setting",
  "description": "插件设置",
  "windows": [
    "plugin-setting"
  ],
  "permissions": [
    "core:default",
    "core:app:allow-set-app-theme",
    "core:window:allow-destroy",
    "dialog:allow-open",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-load",
    "store:allow-delete",
    "store:allow-entries",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "velesdb:default",
    "mcp:default",
    "velesdb:allow-delete-points",
    "log:default"
  ]
}